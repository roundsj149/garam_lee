<mapper
	namespace="com.explorer.routemap.board.mapper.LocationCategorySQLMapper">
	<select id="selectByNo"
		resultType="com.explorer.routemap.board.vo.LocationCategoryVo">
	<![CDATA[
	SELECT *
	FROM RM_LC_Category
	WHERE location_category_no= #{location_category_no}	
	]]>
	</select>

</mapper>