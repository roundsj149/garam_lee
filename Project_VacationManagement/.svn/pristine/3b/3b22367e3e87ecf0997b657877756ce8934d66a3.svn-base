package com.dayside.vacation.vacation.model;

import java.util.Date;

public class Vacation {
	private String vacationApplyNo;
	private String employeeNo;
	private Date applyDt;
	private Date startDt;
	private Date endDt;
	private String startType;
	private String endType;
	private float countDays;
	private String vacationStatus;
	private float beforeCountDays;
	private float afterCountDays;
	
	public Vacation() {
		super();
	}

	public Vacation(String vacationApplyNo, String employeeNo, Date applyDt, Date startDt, Date endDt, String startType,
			String endType, float countDays, String vacationStatus, float beforeCountDays, float afterCountDays) {
		super();
		this.vacationApplyNo = vacationApplyNo;
		this.employeeNo = employeeNo;
		this.applyDt = applyDt;
		this.startDt = startDt;
		this.endDt = endDt;
		this.startType = startType;
		this.endType = endType;
		this.countDays = countDays;
		this.vacationStatus = vacationStatus;
		this.beforeCountDays = beforeCountDays;
		this.afterCountDays = afterCountDays;
	}

	public String getVacationApplyNo() {
		return vacationApplyNo;
	}

	public void setVacationApplyNo(String vacationApplyNo) {
		this.vacationApplyNo = vacationApplyNo;
	}

	public String getEmployeeNo() {
		return employeeNo;
	}

	public void setEmployeeNo(String employeeNo) {
		this.employeeNo = employeeNo;
	}

	public Date getApplyDt() {
		return applyDt;
	}

	public void setApplyDt(Date applyDt) {
		this.applyDt = applyDt;
	}

	public Date getStartDt() {
		return startDt;
	}

	public void setStartDt(Date startDt) {
		this.startDt = startDt;
	}

	public Date getEndDt() {
		return endDt;
	}

	public void setEndDt(Date endDt) {
		this.endDt = endDt;
	}

	public String getStartType() {
		return startType;
	}

	public void setStartType(String startType) {
		this.startType = startType;
	}

	public String getEndType() {
		return endType;
	}

	public void setEndType(String endType) {
		this.endType = endType;
	}

	public float getCountDays() {
		return countDays;
	}

	public void setCountDays(float countDays) {
		this.countDays = countDays;
	}

	public String getVacationStatus() {
		return vacationStatus;
	}

	public void setVacationStatus(String vacationStatus) {
		this.vacationStatus = vacationStatus;
	}

	public float getBeforeCountDays() {
		return beforeCountDays;
	}

	public void setBeforeCountDays(float beforeCountDays) {
		this.beforeCountDays = beforeCountDays;
	}

	public float getAfterCountDays() {
		return afterCountDays;
	}

	public void setAfterCountDays(float afterCountDays) {
		this.afterCountDays = afterCountDays;
	}
}
