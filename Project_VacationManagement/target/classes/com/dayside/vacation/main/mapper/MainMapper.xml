<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dayside.vacation.main.mapper.MainMapper">
	<!-- 코드 그룹 -->
	<select id="selectKoreanCodeName" resultType="String">
	<![CDATA[
			SELECT /* MainMapper.xml	selectKoreanCodeName */
				KOREAN_CODE_NAME 
			FROM 
				DAYSIDE_CODE
			WHERE 
				CODE_ID=#{codeId}
	]]>
	</select>
</mapper>
